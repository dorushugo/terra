{
  "name": "terra",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:vercel": "./scripts/build-vercel.sh",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_DROP_DATABASE=false next dev",
    "dev:stable": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_DROP_DATABASE=false PAYLOAD_DISABLE_ADMIN_HOT_RELOAD=true next dev --turbo",
    "dev:fast": "cross-env NODE_OPTIONS=--no-deprecation PAYLOAD_DROP_DATABASE=false PAYLOAD_DISABLE_ADMIN_HOT_RELOAD=true PAYLOAD_LOG_LEVEL=warn PAYLOAD_DISABLE_SCHEMA_VALIDATION=true next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "./scripts/test-simple.sh",
    "test:unit": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.unit.mts",
    "test:unit:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest --config ./vitest.config.unit.mts",
    "test:unit:coverage": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.unit.mts --coverage",
    "test:integration": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.integration.mts",
    "test:integration:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest --config ./vitest.config.integration.mts",
    "test:integration:coverage": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.integration.mts --coverage",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:e2e:headed": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts --headed",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:all": "./scripts/run-all-tests.sh --all",
    "test:all:coverage": "./scripts/run-all-tests.sh --all --coverage"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.55.1",
    "@payloadcms/db-postgres": "3.55.1",
    "@payloadcms/live-preview-react": "3.55.1",
    "@payloadcms/next": "3.55.1",
    "@payloadcms/payload-cloud": "3.55.1",
    "@payloadcms/plugin-form-builder": "3.55.1",
    "@payloadcms/plugin-nested-docs": "3.55.1",
    "@payloadcms/plugin-redirects": "3.55.1",
    "@payloadcms/plugin-search": "3.55.1",
    "@payloadcms/plugin-seo": "3.55.1",
    "@payloadcms/richtext-lexical": "3.55.1",
    "@payloadcms/storage-vercel-blob": "3.55.1",
    "@payloadcms/ui": "3.55.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "framer-motion": "^12.23.18",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.378.0",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.55.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "stripe": "^18.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "critters": "^0.0.25",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
